= content_tag(:div, flash[:error], :id => "flash_error") if flash[:error]
= content_tag(:div, flash[:notice], :id => "flash_notice") if flash[:notice]
= content_tag(:div, flash[:alert], :id => "flash_alert") if flash[:alert]

= render 'layouts/header'
= render 'layouts/nav'

      
#page-body
  #map 

  = yield

  / # = render 'layouts/contact'


  .container#prepare-container
    =image_tag asset_path('x.png'), :class => 'close'
    = render 'layouts/prepare'
  .container#login-container
    =image_tag asset_path('x.png'), :class => 'close'
    = render 'home/login_modal'
  .container#signin-container
    =image_tag asset_path('x.png'), :class => 'close'
    = render 'home/sign_up_modal'
  .container#advanced-container
    =image_tag asset_path('x.png'), :class => 'close'
    = render 'layouts/advanced'
  .container#recovery-container
    %span.glyphicon.glyphicon-remove
    = render 'layouts/recover'
  .container#account-container
    / - if session[:modal] == true
    =image_tag asset_path('x.png'), :class => 'close'
    = render 'users/personal'

  / render :partial => 'layouts/modal'
  .container#users
    =image_tag asset_path('x.png'), :class => 'close'
    = render 'users/all'
  .container#resilience-container
    =image_tag asset_path('x.png'), :class => 'close'
    = render 'layouts/resiliency'
  .container#print-container
    =image_tag asset_path('x.png'), :class => 'close'
    = render 'layouts/print'
    / .container#print-container
    / =image_tag asset_path('x.png'), :class => 'close'
    / = render 'layouts/print'
